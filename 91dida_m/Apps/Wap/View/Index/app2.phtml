  <title>邸达电子商务有限公司</title>
  <style>
    *{
      margin:0;
      padding: 0;
    }

    html, body {
      background-color: #fff;
    }
  	body {
      margin: 0;
      -webkit-overflow-scrolling: touch;
      -webkit-box-sizing: border-box;
      -moz-box-sizing: border-box;
      box-sizing: border-box;
      word-wrap: break-word;
      color: #333;
      font: 14px/1.3 '\5FAE\8F6F\96C5\9ED1', STXihei, '\534E\6587\7EC6\9ED1', Roboto, Noto, arial, sans-serif;
      background-color: #f0f0f0;
    }
    a {
      color: inherit;
      text-decoration: none;
    }
    h4{
      font-size: 14px;
      font-weight: normal;
    }
    .swipe-not-found {
      padding-top: 49%;
      background: url(//static.91dida.cn/skin/m/images/banner.png) 50% 50% no-repeat;
      -webkit-background-size: cover;
      background-size: cover;
    }
    ::-webkit-scrollbar {
      display: none;
    }

    .wrapper {
      width: 100%;
      max-width: 720px;
      text-align: left;
      display: inline-block;
      position: relative;
      -webkit-box-sizing: border-box;
      -moz-box-sizing: border-box;
      box-sizing: border-box;
      padding-bottom:10px;
    }

    em[class^='icon-'] {
      display: inline-block;
      background-repeat: no-repeat;
      background-image: url(/src/images/icon_sprite.png);
      -webkit-background-size: 289px 274px;
      background-size: 289px 274px;
    }

    .icon-add-carts {
      background-position: -216px -175px;
      width: 25px;
      height: 25px;
    }

    .icon-recommend-title {
      background-position: -189.5px 0;
      width: 24px;
      height: 90px;
    }

    .icon-home-shop-more {
      background-position: -123px -102px;
      width: 38px;
      height: 12.5px; }
    .icon-home-shop-more.open {
        background-position: -47.5px -161.5px;
    }

    .swipe-wrapper {
      position: relative;
      overflow: hidden;
      background-color: #fff;
    }

    .swipe-wrapper .swipe-container {
      position: absolute;
      left: 0;
      top: 0;
      right: 0;
      bottom: 0;
      overflow: hidden;
      list-style: none;
    }

    .swipe-wrapper a {
      display: block;
      width: 100%;
      height: 100%;
      background-repeat: no-repeat;
      background-position: 50% 50%;
      background-size: cover;
    }

    .swipe-wrapper .padding a {
      background-size: 80%;
    }

    .swipe-wrapper .swipe-point {
      position: absolute;
      z-index: 1;
      right: 10px;
      bottom: 5px;
    }

    .swipe-wrapper .swipe-point em {
      display: inline-block;
      width: 8px;
      height: 8px;
      margin: 0 5px;
      border-radius: 20px;
      border: 1px solid #fff;
    }

    .swipe-wrapper .swipe-point .active {
      background-color: #fff;
    }

    .swipe-wrapper .banner {
      float: left;
      position: relative;
      width: 100%;
      height: 100%;
    }

    .product-item-wrapper {
      font-size: 0;
    }

    .product-item-wrapper .item {
      width: 49%;
      position: relative;
      display: inline-block;
      box-sizing: border-box;
      padding: 10px;
      margin-top: 2%;
      background-color: #fff;
      overflow: hidden;
    }

    .product-item-wrapper .item:nth-child(1) {
      margin-top: 0;
    }

    .product-item-wrapper .item:nth-child(2) {
      margin-top: 0;
    }

    .product-item-wrapper .item:nth-child(odd) {
      margin-right: 1%;
    }

    .product-item-wrapper .item:nth-child(even) {
      margin-left: 1%;
    }

    .product-item-wrapper .item a {
      display: block;
    }

    .product-item-wrapper .topic {
      display: block;
      width: 80%;
      padding-top: 80%;
      margin: 0 auto;
      background-repeat: no-repeat;
      background-position: 50% 50%;
      margin-bottom: 10px;
      background-size: cover;
    }

    .product-item-wrapper .name {
      font-size: 14px;
      margin-bottom: 5px;
      overflow: hidden;
      white-space: nowrap;
      text-overflow: ellipsis;
    }

    .product-item-wrapper .price {
      color: #fd4d3e;
      font-size: 14px;
    }

    .product-item-wrapper .add-carts {
      position: absolute;
      right: 0;
      bottom: 0;
      z-index: 1;
      padding: 10px;
    }

    .product-item-wrapper .goods-stock {
      color: #fd4d3e;
      font-size: 12px;
      position: absolute;
      right: 10px;
      bottom: 12px;
    }

    .home-category {
      background-color: #fff;
      font-size: 0;
      padding: 15px 0;
    }

    .home-category .item {
      text-align: center;
      font-size: 14px;
      display: inline-block;
      width: 25%;
      margin-bottom: 10px;
    }

    .home-category .item:nth-child(5) ~ a {
      margin-bottom: 0;
    }

    .home-category .icon {
      display: block;
      margin: 0 auto;
      margin-bottom: 5px;
      width: 60%;
    }

    .home-shop article {
      padding: 10px 0 0;
      background-color: #fff;
    }

    .home-shop .item {
      position: relative;
      display: block;
      overflow: hidden;
      padding: 8px 3% 8px;
    }

    .home-shop .item:before {
      content: '';
      height: 1px;
      position: absolute;
      left: 3%;
      right: 0;
      -webkit-transform: scaleY(0.5);
      transform: scaleY(0.5);
      z-index: 1;
      top: 0;
      background-color: #e5e5e5;
    }

    .home-shop .item:first-child:before {
      display: none;
    }

    .home-shop .topic {
      float: left;
      width: 60px;
      height: 60px;
      border-radius: 5px;
      overflow: hidden;
      background-repeat: no-repeat;
      background-position: 50% 50%;
      margin-right: 10px;
      background-size: cover;
    }

    .home-shop h4.title {
      margin-top: 3px;
      height: 35px;
      overflow: hidden;
    }

    .home-shop .num {
      overflow: hidden;
      font-size: 12px;
    }

    .home-shop .num span {
      display: inline-block;
      vertical-align: middle;
    }

    .home-shop .sales {
      width: 40%;
      color: #ed722c;
      margin-right: 5px;
    }

    .home-shop .more {
      display: block;
      padding-top: 10px;
      width: 50px;
      margin: -8px auto 0;
      font-size: 0;
      height: 12px;
      overflow: hidden;
      text-align: center;
    }

    .home-title-wrapper {
      display: block;
      position: relative;
      text-align: center;
      padding: 10px 10px 8px;
      overflow: hidden;
    }

    .home-title-wrapper em[class^='icon-'] {
      vertical-align: middle;
    }

    .home-title-wrapper .text {
      font-size: 16px;
      margin-left: 5px;
      vertical-align: middle;
    }

    .home-title-wrapper .icon-more:before {
      width: 8px;
      height: 8px;
      border-color: #666;
    }

    .home-title-wrapper .title {
      position: relative;
      display: inline-block;
      padding: 0 10px;
      width: 50%;
    }

    .home-title-wrapper .title:before {
      content: '';
      position: absolute;
      top: 50%;
      left: -10%;
      width: 30%;
      height: 1px;
      background-color: currentColor;
    }

    .home-title-wrapper .title:after {
      content: '';
      position: absolute;
      top: 50%;
      right: -10%;
      width: 30%;
      height: 1px;
      background-color: currentColor;
    }

    .home-title-wrapper .more-text {
      font-size: 12px;
      position: absolute;
      right: 20px;
      top: 50%;
      height: 14px;
      margin-top: -7px;
    }

    .home-brand article {
      background-color: #fff;
      font-size: 0;
      position: relative;
    }

    .home-brand article a {
      display: inline-block;
      vertical-align: middle;
    }

    .home-brand .scroll {
      padding-left: 25px;
      overflow-x: auto;
    }

    .home-brand .item {
      width: 90px;
      height: 90px;
      background-repeat: no-repeat;
      background-position: 50% 50%;
      margin-right: 10px;
      background-size: cover;
    }

    .home-brand .view-more {
      width: 30px;
      height: 75px;
      padding-top: 15px;
    }

    .home-brand .view-more span {
      display: inline-block;
      vertical-align: middle;
      width: 16px;
      font-size: 12px;
      margin-left: 3px;
    }

    .home-brand .recommend-title {
      position: absolute;
      left: 0;
      top: 0;
      height: 100%;
      z-index: 1;
    }

    .home-new-products article {
      background-color: #fff;
      font-size: 0;
    }

    .home-new-products .item {
      width: 33.33%;
      text-align: center;
      position: relative;
      display: inline-block;
      box-sizing: border-box;
      padding: 10px;
    }

    .home-new-products .item:after {
      content: '';
      height: 1px;
      position: absolute;
      left: 0;
      right: 0;
      -webkit-transform: scaleY(0.5);
      transform: scaleY(0.5);
      z-index: 1;
      bottom: 0;
      background-color: #e5e5e5;
    }

    .home-new-products .item:before {
      content: '';
      width: 1px;
      position: absolute;
      top: 0;
      bottom: 0;
      -webkit-transform: scaleX(0.5);
      transform: scaleX(0.5);
      z-index: 1;
      right: 0;
      background-color: #e5e5e5;
    }

    .home-new-products .topic {
      display: inline-block;
      width: 80%;
      padding-top: 80%;
      background-repeat: no-repeat;
      background-position: 50% 50%;
      margin-bottom: 10px;
      background-size: cover;
    }

    .home-new-products .name {
      overflow: hidden;
      text-overflow: ellipsis;
      white-space: nowrap;
      font-size: 14px;
      margin-bottom: 5px;
    }

    .home-new-products .price {
      font-size: 14px;
      text-align: left;
      color: #fd4d3e;
    }

    .home-new-products .new-product-ad {
      padding: 0 10px;
      overflow: hidden;
    }

    .home-new-products .new-product-ad a {
      background-repeat: no-repeat;
      background-position: 50% 50%;
      background-size: cover;
      float: left;
    }

    .home-new-products .new-product-ad a:first-child {
      width: 50%;
      padding-top: 42%;
      margin-right: 2%;
    }

    .home-new-products .new-product-ad a:nth-child(2), .home-new-products .new-product-ad a:nth-child(3) {
      width: 48%;
      padding-top: 20%;
    }

    .home-new-products .new-product-ad a:nth-child(2) {
      margin-bottom: 2%;
    }
  </style>
</head>
<body>
<div class="wrapper">
  <if condition='$items["adv"]'>
    <aside class="swipe-wrapper" id="scrollImage" style="padding-top:49%">
      <ul class="swipe-container">
        <foreach name="items['adv']" item="adv">
          <li class="banner">
            <if condition='($adv["link_type"] eq "url") OR ($adv["link_type"] eq "activity")' >
              <a href="javascript:DIDAJSBridge.callHandler('OPEN_WEBVIEW', { tagName: 'open_webview', url: '{$adv['link']}' });" style="background-image: url({$adv['file']});"></a>
            </if>
            <if condition='$adv["link_type"] eq "products"'>
              <a href="javascript:DIDAJSBridge.callHandler('GOODS_DETAIL', { tagName: 'goods_detail', goodsId: {$adv['link']}, storeId:0 }); style="background-image: url({$adv['file']});"></a>
            </if>

          </li>
        </foreach>
      </ul>
      <div class="swipe-point">
        <foreach name="items['adv']" item="adv">
          <em class="point"></em>
        </foreach>
      </div>
    </aside>
  <else />
    <div class="swipe-not-found"></div>
  </if>

  <if condition='$items["category"]'>
    <section class="home-category">
      <foreach name="items['category']" item="category">
        <a href="javascript:DIDAJSBridge.callHandler('GOODS_LIST', { tagName: 'goods_list', type: 'category', categoryId: {$category['category_id']} });" class="item">
          <img src="{$category['app_icon']}" class="icon">
          <p>{$category['category_name']}</p>
        </a>
      </foreach>
    </section>
  </if>

  <if condition="$items['store_is_recommend']">
    <section class="home-shop">
      <a href="javascript:DIDAJSBridge.callHandler('SHOP_LIST', { tagName: 'shop_LIST' });" class="home-title-wrapper">
        <div class="title" style="color: rgb(246, 193, 87);">
          <em class="icon-32 icon-home-shop"></em>
          <span class="text">商家推荐</span>
        </div>
        <span class="more-text">更多</span>
        <em class="icon-more"></em>
      </a>
      <article>
        <foreach name="items['store_is_recommend']" item="recommend_store">
          <a href="javascript:DIDAJSBridge.callHandler('SHOP_HOME', { tagName: 'shop_home', shopId: {$recommend_store['store_id']} });" class="item">
            <span class="topic" style="background-image: url({$recommend_store['logo']});"></span>
            <h4 class="title">{$recommend_store['store_name']}</h4>
            <p class="num">
              <span class="sales">起送金额 {$recommend_store['starting_price']} 元</span>
              <span class="godds">商品:{$recommend_store['items_num']}款</span>
            </p>
          </a>
        </foreach>
        <if condition="$items['store_is_recommend']|count gt 3 ">
          <a class="more">
            <em class="icon-home-shop-more open"></em>
          </a>
        </if>
      </article>
    </section>
  </if>

  <if condition="$items['brand']">
    <section class="home-brand">
      <a href="javascript:DIDAJSBridge.callHandler('BRAND_LIST', { tagName: 'brand_list' });" class="home-title-wrapper">
        <div class="title" style="color: rgb(147, 106, 217);">
          <em class="icon-32 icon-home-brand"></em>
          <span class="text">品牌推荐</span>
        </div>
        <span class="more-text">更多</span>
        <em class="icon-more"></em>
      </a>
      <article>
