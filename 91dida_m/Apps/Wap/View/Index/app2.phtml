  <if condition="$items['special_offer']">
    <section class="home-products">
      <a href="javascript:DIDAJSBridge.callHandler('GOODS_LIST', { tagName: 'goods_list',type: 'bargain' });" class="home-title-wrapper">
        <div class="title" style="color: rgb(93, 209, 102);">
          <em class="icon-32 icon-home-dis"></em>
          <span class="text">特价商品</span>
        </div>
        <span class="more-text">更多</span>
        <em class="icon-more"></em>
      </a>
      <ul class="product-item-wrapper">
        <foreach name="items['special_offer']" item="specialOffer">
          <li class="item">
            <a href="javascript:DIDAJSBridge.callHandler('GOODS_DETAIL', { tagName: 'goods_detail', goodsId: {$specialOffer['store_product_entity_id']}, storeId:{$specialOffer['store_product_store_id']} });" class="product">
              <span class="topic" style="background-image: url({$specialOffer['default_media_gallery_thumb']});"></span>
              <h4 class="name">{$specialOffer['product_name']}</h4>
              <p class="price">¥{$specialOffer['store_product_final_price']}</p>
            </a>
            <if condition="$specialOffer['store_product_stock_qty'] neq 0">
              <a href="javascript:DIDAJSBridge.callHandler('ADD_TO_CART', { tagName: 'add_to_cart', shopId: {$specialOffer['store_product_store_id']}, productId: {$specialOffer['store_product_entity_id']} });" class="add-carts">
                <em class="icon-add-carts"></em>
              </a>
            <else />
              <span class="goods-stock">补货中</span>
            </if>
          </li>
        </foreach>
      </ul>
    </section>
  </if>
</div>
<script>
  ;(function(document) {
    /*图片轮播*/
    if(document.getElementById("scrollImage")){
      document.getElementById("scrollImage").querySelectorAll(".point")[0].classList.add("active");
      new Swipe(document.getElementById("scrollImage"), {
        auto: 3000,
        autoRestart: true,
        transitionEnd: function(index) {
          document.getElementById("scrollImage").querySelector(".active").classList.remove("active");
          document.getElementById("scrollImage").querySelectorAll(".point")[index].classList.add("active");
        }.bind(this)
      });
    }

    if(document.querySelector(".scroll-bar")){
      var scrollBarContainer = document.querySelector(".scroll-bar"),
        barListLength = scrollBarContainer.children.length,
        onceWidth = scrollBarContainer.children[0].innerWidth;

      scrollBarContainer.style.width = barListLength*onceWidth + "px";
    }

    if(document.querySelector(".icon-home-shop-more")){
      var shopList = document.querySelector(".home-shop").querySelectorAll(".item");

      for(var i = 3, l = shopList.length; i < l; i++){
        shopList[i].style.display = "none";
      }

      document.querySelector(".icon-home-shop-more").addEventListener("click", function() {
        var opened = this.classList.contains("open"),
          shopList = document.querySelector(".home-shop").querySelectorAll(".item");

        for(var i = 3, l = shopList.length; i < l; i++){
          shopList[i].style.display = opened ? "block" : "none";
        }

        if(opened){
          this.classList.remove("open");
        } else {
          this.classList.add("open");
        }

      })
    }
  })(document);
</script>
</body>
</html>
