        <div class="title" style="color: rgb(252, 86, 113);">
          <em class="icon-32 icon-home-hot"></em>
          <span class="text">热销商品</span>
        </div>
        <span class="more-text">更多</span>
        <em class="icon-more"></em>
      </a>
      <ul class="product-item-wrapper">
        <foreach name="items['hot']" item="hot">
          <li class="item">
            <a href="javascript:DIDAJSBridge.callHandler('GOODS_DETAIL', { tagName: 'goods_detail', goodsId: {$hot['store_product_entity_id']}, storeId:{$hot['store_product_store_id']} });" class="product">
              <span class="topic" style="background-image: url({$hot['default_media_gallery_thumb']});"></span>
              <h4 class="name">{$hot['product_name']}</h4>
              <p class="price">¥{$hot['store_product_final_price']}</p>
            </a>
            <if condition="$hot['store_product_stock_qty'] neq 0">
              <a href="javascript:DIDAJSBridge.callHandler('ADD_TO_CART', { tagName: 'add_to_cart', shopId: {$hot['store_product_store_id']}, productId: {$hot['store_product_entity_id']} });" class="add-carts">
                <em class="icon-add-carts"></em>
              </a>
            <else />
              <span class="goods-stock">补货中</span>
